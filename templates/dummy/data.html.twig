{% extends 'base.html.twig' %}

{% block title %}Hello DummyController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>{{ measurementIndex.title }}</h1>
    <p>{{ measurementIndex.description }}</p>
    <h1>¿Qué medimos?</h1>
    <p>{{ measurementIndex.whatWeMeasure }}</p>
    <h1>¿Cómo lo medimos?</h1>
    <p>{{ measurementIndex.howWeMeasure }}</p>
    <hr>
    <p>Datos recoletados:</p>

    {% for surveySubmit in data %}
      {{ surveySubmit.submittedData|json_encode() }}
      <br>
    {% endfor %}
</div>
{% endblock %}
